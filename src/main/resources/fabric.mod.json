{
  "schemaVersion": 1,
  "id": "addon-template",
  "version": "${version}",
  "name": "MUtils",
  "description": "An addon template for Meteor Client addons.",
  "authors": [
    "Marccccccccccccccc"
  ],
  "contact": {
    "sources": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "marc3d.mutils.MUtils"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "0,255,0"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}", "1.21.7", "1.21.6"],
    "meteor-client": "*"
  }
}
